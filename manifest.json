{
  "name": "七牛云存储",
  "short_name": "七牛云存储",
  "description": "绑定七牛账号即可右键菜单或者拖拽方式上传文件到云端",
  "version": "1.0.0",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "contextMenus"
  ],
  "background": {
    "scripts": ["js/menu.js"]
  },
  "browser_action": {
    "default_title": "七牛云图床",
    "default_icon": "icons/128.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "homepage_url": "https://github.com/puge/qiniu",
  "options_page": "options.html"
}